        <add allowed="true" ipAddress="101.178.141.127"/>
        <add allowed="true" ipAddress="108.171.134.163"/>
        <add allowed="true" ipAddress="108.171.133.163"/>
        <add allowed="true" ipAddress="202.128.106.33"/>
      </ipSecurity>
    </security>
    
    <httpErrors xdt:Transform="Remove"/>    
  </system.webServer>
  
</configuration>